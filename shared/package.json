    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.5"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
